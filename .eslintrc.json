{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": ["google"],
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "no-invalid-this": "off",
    "no-unused-vars": 0,
    "linebreak-style": "off",
    "no-console": 1,
    "require-jsdoc": 0,
    "react/jsx-uses-vars": 0,
    "react/prefer-stateless-function": "off",
    "react/prop-types": 1,
    "react/no-danger": "off",
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "hrefLeft", "hrefRight", "to" ],
      "aspects": [ "noHref", "invalidHref", "preferButton" ]
    }]
  },
  "settings": {
    "import/core-modules": []
  },
  "env": {
    "browser": true
  }
}
